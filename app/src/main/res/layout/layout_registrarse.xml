                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:text="@string/producto2"
                    android:textSize="@dimen/letra_s3" />

                <me.originqiu.library.EditTag
                    android:id="@+id/et_sector_producto_registro"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/producto2"
                    android:textColorHint="@color/celeste"
                    android:textSize="@dimen/letra_s4"
                    app:delete_mode_bg="@color/plomo_oscuro"
                    app:input_layout="@layout/view_sample_input_tag"
                    app:tag_layout="@layout/layout_tag" />

            </LinearLayout>
        </LinearLayout>

