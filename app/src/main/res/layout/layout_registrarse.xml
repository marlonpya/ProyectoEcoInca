                    android:layout_height="match_parent"
                    android:paddingTop="10dp">

                    <EditText
                        android:id="@+id/et_contrasenia_registro"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="@string/contrasenia"
                        android:inputType="textPassword"
                        android:textColorHint="@color/celeste"
                        android:textSize="@dimen/letra_s4" />
                </android.support.design.widget.TextInputLayout>
            </LinearLayout>

        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tipo_usuario"
            android:textColor="@color/plomo_oscuro"
            android:textSize="@dimen/letra_s5" />

        <include layout="@layout/tres_opciones" />

        <LinearLayout
